# Kamal secrets - copy to .kamal/secrets and fill in values

# Container registry
KAMAL_REGISTRY_PASSWORD=ghp_xxx

# Database
DATABASE_URL=postgresql://loop:password@10.0.1.30:5432/loop
POSTGRES_PASSWORD=your-secure-postgres-password
POSTGRES_USER=loop
POSTGRES_PWD=your-secure-postgres-password

# Application secrets
JWT_SECRET=your-super-secret-jwt-key-at-least-32-chars
ENCRYPTION_KEY=your-encryption-key-at-least-32-chars

# OpenObserve
ZO_ROOT_USER_EMAIL=admin@loop.dev
ZO_ROOT_USER_PASSWORD=your-secure-openobserve-password
